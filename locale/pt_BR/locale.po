msgid "plugins.generic.CspSubmission.displayName"
msgstr "Submissão CSP"

msgid "plugins.generic.CspSubmission.description"
msgstr "Customiza a etapa de submissão de acordo como o fluxo do Cadernos de Saúde Pública da Fiocruz."

msgid "plugins.generic.CspSubmission.espacoTematico"
msgstr "Tema"

msgid "plugins.generic.CspSubmission.conflitoInteresse"
msgstr "Seu artigo possui conflito de interesse?"

msgid "plugins.generic.CspSubmission.codigoFasciculoTematico"
msgstr "Código do fascículo temático"

msgid "plugins.generic.CspSubmission.codigoFasciculoTematico.description"
msgstr "Preencha este campo caso seu artigo pertença a um volume temático (solicite o código ao editor convidado)"

msgid "plugins.generic.CspSubmission.agradecimentos"
msgstr "Agradecimentos"

msgid "plugins.generic.CspSubmission.codigoArtigoRelacionado"
msgstr "Código"

msgid "plugins.generic.CspSubmission.SectionFile.errorWordCount"
msgstr "A sessão "{$sectoin}" deve conter texto com no máximo {$max} palavras. O arquivo enviado contém {$count} palavras."

msgid "plugins.generic.CspSubmission.SectionFile.invalidFormat.AticleBody"
msgstr "Formato de arquivo inválido! Formatos permitidos para este tipo de arquivo: .doc, .docx ou .odt"

msgid "plugins.generic.CspSubmission.SectionFile.invalidFormat.Image"
msgstr "Formato de arquivo inválido! Formatos permitidos para este tipo de arquivo: .png, .jpeg, .bmp, .tiff, .xls, .xlsx ou .ods"

msgid "plugins.generic.CspSubmission.keywords.Notification"
msgstr "São necessárias no mínimo 3 e no máximo 5 palavras-chave"

msgid "plugins.generic.CspSubmission.submission.bodyTextFile.limit"
msgstr "Só é permitido inseir um arquivo Corpo do texto"

msgid "plugins.generic.CspSubmission.submission.transcriptsFile.limit"
msgstr "Só é permitido inseir um arquivo de legendas"

msgid "plugins.generic.CspSubmission.consideracoesEticas"
msgstr "Considerações éticas e legais"

msgid "plugins.generic.CspSubmission.consideracoesEticas.checkbox.yes"
msgstr "No caso de artigos que envolvem pesquisas com seres humanos, foram cumpridos os princípios contidos na Declaração de Helsinki, além de atendida a legislação específica do país no qual a pesquisa foi realizada. No caso de pesquisa envolvendo animais da fauna silvestre e/ou cobaias foram atendidas as legislações pertinentes."

msgid "plugins.generic.CspSubmission.consideracoesEticas.checkbox.no"
msgstr "Não se aplica (estudo não envolve pesquisa com seres humanos ou animais)."

msgid "plugins.generic.CspSubmission.submission.newVersion.alert"
msgstr "É obrigatória a submissão de uma carta ao editor associado escolhendo o componente <b>Alterações realizadas</b>"

msgid "plugins.generic.CspSubmission.submission.title.description"
msgstr "Limite mínimo 15, limite máximo 300 caracteres com espaços"
